{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System": "Warning"
        }
    },
    "Serilog": {
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "CsScript": {
        "TargetFramework": "net8.0",
        "CacheDirectory": "{WorkingDir}/csx_cache"
    },
    "OneScript": {
        "LibDirectory": ""
    }
}